<odoo>
    <record id="inv_ext_field" model="ir.ui.view">
        <field name="name">account.move.form</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">

        <!-- <header> 
        <button string="Compute Commision" name="action_check" type="object" class="oe_highlight"/>

        </header> -->

            <xpath expr="//field[@name='amount_total']" position="attributes">

                        <attribute name="string">Commision Amount</attribute>

            </xpath>

            <xpath expr="//field[@name='amount_untaxed']" position="attributes">

                        <attribute name="string">Discount By Avennue</attribute>

            </xpath>

            <xpath expr="//field[@name='payment_reference']" position="after">
            
                        <field name="project_name" />
                        <field name="plot_size" />
                        <field name="internal_ref" />
                        <field name="price" />            
            
            </xpath>

            <xpath expr="//label[@for='journal_id']" position="after">
            
                        <field name="additional_price" />
                        <field name="cost" />
                        <field name="discount_builder" />
                        <field name="gross_amount" />              
            
            </xpath>

        </field>

    </record>
</odoo>